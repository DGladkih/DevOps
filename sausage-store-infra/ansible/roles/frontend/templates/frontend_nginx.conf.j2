server {
    listen 80;
    server_name localhost;

    root /var/www-data/frontend/dist/frontend;
    index index.html;

    location / {
    try_files $uri $uri/ /index.html =404;
}

    location /static/ {
        expires 1y;
        add_header Cache-Control "public, immutable, no-transform";
    }

    error_page  404              /index.html;
}
